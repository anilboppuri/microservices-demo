spring:
  application:
    name: hello-client
server:
       port: 8092

eureka:
  client:
    register-with-eureka: true
    fetch-registry: true
    service-url:
         defaultZone: http://localhost:8090/eureka
  instance: 
    hostname: localhost       
    
 
spring.cloud.config.uri=http://localhost:8888
